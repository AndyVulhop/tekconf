@model dynamic

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">

    <title>Api Documentation - conferences.io</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">

    <link href="~/css/bootstrap.css" rel="stylesheet">
    <link href="~/css/bootstrap-responsive.css" rel="stylesheet">

    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,600" rel="stylesheet">
    <link href="~/css/font-awesome.css" rel="stylesheet">

    <link href="~/css/reboot-landing.css" rel="stylesheet">
    <link href="~/css/reboot-landing-responsive.css" rel="stylesheet">

    <link href="~/css/themes/green/theme.css" rel="stylesheet">
    <link href="~/css/pages/faq.css" rel="stylesheet">

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
<![endif]-->

</head>

<body>
    @Html.Partial("Header")

    <div id="subheader">
        <div class="inner">
            <div class="container">
                <h1>My Schedule</h1>
            </div>
            <!-- /.container -->
        </div>
        <!-- /inner -->
    </div>
    <!-- /subheader -->

    <div id="subpage">
        <div class="container">
            <div class="row">
                <div class="span8">
                    <h3><span class="slash">//</span> Conferences</h3>
                    <table class="table">
                        <thead>
                            <tr>
                                <td>Resource
                                </td>

                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>GET v1/conferences/
                                </td>
                            </tr>
                            <tr>
                                <td>GET v1/conferences/&lt;conference_slug&gt;
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!-- /span8 -->

                <!-- /span4 -->
            </div>
            <!-- /row -->



        </div>
        <!-- /container -->
    </div>
    <!-- /subpage -->

    <div id="extra">
        <div class="inner">
            <div class="container">
                <div class="row">
                    <div class="span4">
                        <h3><span class="slash">//</span> Quick Links</h3>

                        <ul class="footer-links clearfix">
                            <li><a href="~/">Home</a></li>
                            <li><a href="~/Conferences">Conferences</a></li>
                            <li><a href="~/Schedule">My Schedule</a></li>
                        </ul>

                        <ul class="footer-links clearfix">
                            <li><a href="~/Api">Api</a></li>
                            <li><a href="~/Apps">Apps</a></li>
                        </ul>

                    </div>
                    <!-- /span4 -->

                    <div class="span4">
                        <h3><span class="slash">//</span> Stay In Touch</h3>

                        <p>There are real people behind conferences.io, so if you have a question or suggestion (no matter how small) please get in touch with us:</p>
                        <ul class="social-icons-container">
                            <li>
                                <a href="javascript:;" class="social-icon social-icon-twitter">Twitter
                                </a>
                            </li>

                            <li>
                                <a href="javascript:;" class="social-icon social-icon-googleplus">Google +
                                </a>
                            </li>

                            <li>
                                <a href="javascript:;" class="social-icon social-icon-facebook">Facebook
                                </a>
                            </li>
                        </ul>
                        <!-- /extra-social -->
                    </div>
                    <!-- /span4 -->
                    <div class="span4">
                        <h3><span class="slash">//</span> Subscribe and get updates</h3>

                        <p>Subscribe to our newsletter and get exclusive deals you wont find anywhere else straight to your inbox!</p>
                        <form>
                            <input type="text" name="subscribe_email" placeholder="Your Email" />
                            <br />
                            <button class="btn ">Subscribe</button>
                        </form>
                    </div>
                    <!-- /span4 -->
                </div>
                <!-- /row -->
            </div>
            <!-- /container -->
        </div>
        <!-- /inner -->
    </div>
    <!-- /extra -->

    @Html.Partial("Footer")

    <script src="./js/jquery-1.7.2.min.js"></script>
    <script src="./js/bootstrap.js"></script>
    <script src="./js/faq.js"></script>

    <script>
        $(function () {
        });
    </script>
</body>
</html>
